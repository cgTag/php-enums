{
  "name": "cgtag/php-enums",
  "description": "A tiny library that adds Enums to PHP",
  "keywords": [
    "php",
    "enum",
    "library"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "cgTag",
      "homepage": "https://cgTag.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/cgTag/php-enums/issues",
    "source": "https://github.com/cgTag/php-enums"
  },
  "require": {
    "php": ">=7.1.0",
    "cgtag/php-exceptions": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7,<6"
  },
  "autoload": {
    "psr-4": {
      "cgTag\\Enums\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "cgTag\\Enums\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --no-coverage",
    "coverage": "phpunit"
  }
}
